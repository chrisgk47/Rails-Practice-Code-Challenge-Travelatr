<h1>Create New Post</h1>

<%= form_for @post do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <br>
    <%= f.label :content %>
    <%= f.text_field :content %>
    <br>
    <%= f.label :blogger %>
    <%= f.collection_select :blogger_id, Blogger.all, :id, :name %>
    <br>
    <%= f.label "Destination" %>
    <%= f.collection_select :destination_id, Destination.all, :id, :name %>

    <%= f.submit %>
    



<% end %>
